#include <iostream>
#include <string>
#include <vector>
using namespace std;

int main() {
    string s = "abcabcbb";
    vector<int> lastIndex(256, -1);
    int start = 0, maxLen = 0;

    for(int i=0; i<s.size(); i++) {
        if(lastIndex[s[i]] >= start)
            start = lastIndex[s[i]] + 1;  
        lastIndex[s[i]] = i;
        maxLen = max(maxLen, i - start + 1);
    }

    cout << "Longest unique substring length = " << maxLen;
}
