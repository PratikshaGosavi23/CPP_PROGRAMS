vector<int> generateFibonacciNumbers(int n) {
    vector<int> ans;     // store the sequence
    
    if (n >= 1) ans.push_back(0);   // first number
    if (n >= 2) ans.push_back(1);   // second number
    
    for (int i = 2; i < n; i++) {
        ans.push_back(ans[i - 1] + ans[i - 2]);  // next = sum of last 2
    }
    
    return ans;
}
